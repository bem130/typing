<Page x:Class="typing.PlayPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:typing"
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="800"
      Title="Play"
      Background="{DynamicResource cBackground}">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="elements.xaml"/>
                <ResourceDictionary Source="colorthemes/default.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>

    <Grid>
        <GroupBox x:Name="PauseMenu" Panel.ZIndex="1" Visibility="Hidden" BorderThickness="0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="10"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="10"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="1" Background="{DynamicResource cPauseBackground}" Margin="50,100,50,100">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="85"/>
                        <RowDefinition Height="5*"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="2*"/>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Text="pause" FontSize="65" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource cPauseFontcolor}"/>
                    <Button Grid.Row="2" x:Name="resume_button" Content="resume [Space]" FontSize="30" Width="250" Style="{DynamicResource Button}" Click="Resume_button"/>
                    <Button Grid.Row="4" x:Name="retire_button" Content="retire" FontSize="30" Width="250" Style="{DynamicResource Button}" Click="Retire_button"/>
                </Grid>
            </Grid>
        </GroupBox>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="100*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="230"/>
                <RowDefinition Height="22"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="3*"/>
                    <RowDefinition Height="35"/>
                    <RowDefinition Height="2*"/>
                    <RowDefinition Height="37"/>
                    <RowDefinition Height="40"/>
                    <RowDefinition Height="3*"/>
                    <RowDefinition Height="40"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="6*"/>
                </Grid.RowDefinitions>

                <TextBlock x:Name="Qtitle" Text="" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="35" TextAlignment="Center" Foreground="{DynamicResource cFontcolor}"/>

                <Button x:Name="Qstopwatch" Content="00:00:00" Grid.Row="3" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="25" Height="37" Style="{DynamicResource Button}" Click="Pause_button"/>
                <ProgressBar x:Name="Aprogress" Grid.Row="4" Width="600" Height="15" Background="{DynamicResource cPbarBackground}" Foreground="{DynamicResource cPbarForeground}"></ProgressBar>

                <TextBlock x:Name="Qarea" FontFamily="Noto Sans Light,Noto Sans JP" Grid.Row="6" FontSize="30"  TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource cFontcolor}">
                <Run Text="スペースを押してスタート"/>
                </TextBlock>


                <TextBlock x:Name="Aarea" FontFamily="Noto Sans Light,Noto Sans JP" Grid.Row="8" FontSize="30"  TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource cFontcolor}">
                </TextBlock>
            </Grid>

            <StatusBar Grid.Row="4">
                <TextBlock Text="問"/>
                <TextBlock x:Name="QAnowcnt" Text="-"/>
                <TextBlock Text="/"/>
                <TextBlock x:Name="QAallcnt" Text="-"/>
                <Separator/>
                <TextBlock Text="タイプ"/>
                <TextBlock x:Name="Qtypespeed" Text="0"/>
                <TextBlock Text="/s"/>
                <Separator/>
                <TextBlock Text="ミスタイプ"/>
                <TextBlock x:Name="QAmiscnt" Text="-"/>
                <TextBlock Text="/"/>
                <TextBlock x:Name="QAtypecnt" Text="-"/>
                <Separator/>
                <TextBlock Text="問題"/>
                <TextBlock Text="file:"/>
                <TextBlock x:Name="QAfilename" Text="----"/>
                <TextBlock Text=";"/>
                <TextBlock Text="line:"/>
                <TextBlock x:Name="QAlinecnt" Text="--"/>
                <TextBlock Text="type:"/>
                <TextBlock x:Name="QAtype" Text="--"/>
                <Separator/>
                <TextBlock Text="キーコード:"/>
                <TextBlock x:Name="Latestkey" Text="-"/>
